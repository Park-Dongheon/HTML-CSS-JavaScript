package edu;

public class Test20 {
	public static void main(String[] args) {
		// 점수 초깃값을 입력한다.
		int score = 90;
		// 성적 문자값을 초기화한다.
		char grade;
		
		// 점수가 90점 이상인지 비교한다
		if(score >= 90) {
			// 90점 이상이라면 'A'문자값를 성적 변수에 저장한다.
			grade = 'A';
		} else if(score >= 80) {
			grade = 'B';
		} else if(score >= 70) {
			grade = 'C';
		} else if(score >= 60) {
			grade = 'D';
		} else {
			grade = 'F';
		}
		
		System.out.println(grade);
	}
}
