package edu;

public class Test38 {
	public static void main(String[] args) {
//		int arr[][] = {{1,2}, {3,4}};
		int arr[][] = new int[5][5];
		
		int n = 0;
		
		for (int i = 0; i < arr.length; i++) {
			for (int j = 0; j < arr.length; j++) {
				arr[i][j] = n++;			}
		}
		
		for (int i = 0; i < arr.length; i++) {
			for (int j = 0; j < arr.length; j++) {
				System.out.println(n);
			}
		}
		
		
		
