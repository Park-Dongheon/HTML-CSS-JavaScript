package edu;

import java.util.Scanner;

public class Test26 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int i = sc.nextInt();
		System.out.println("정수값을 입력하세요:");
		
		while(i < 10) {
			System.out.println(i);
			i++;
		}
		
		System.out.println("OK");
		
	}
}
