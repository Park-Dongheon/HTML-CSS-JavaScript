package com.ruby.java.test1;

public class Cylinder {
	
	
	
	int radius = 5;
	int height = 10;
	
//	void setData(int r, int h) {
//		radius = r;
//		height = h;
//	}
	
	double getVolume() {
		return Math.PI * radius * radius * height;
		
	}
	
	double getArea() {
		return Math.PI * radius * radius * 2 + (2 * Math.PI * radius) * height;
		System.out.println("원기둥의 겉넓이: " + area);
	}
	
}
